{
  "version": 2,
  "buildCommand": "pip install -r requirements.txt",
  "devCommand": "streamlit run app.py --server.port $PORT",
  "installCommand": "pip install -r requirements.txt",
  "framework": null,
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app.py"
    }
  ]
}

